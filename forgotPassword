        <script type="text/javascript">

            $(document).ready(function(){
                     
                    jQuery('.dropdown-toggle').dropdown();

                     $(document).on("click","button.rb_usr_book_dtl_btn",function(e){

                        var bid=$(this).attr("data-booking-id");
                          $.ajax({
                              url: Settings.base_url+'Home/customer_requestforbookingdetail',
                              type: 'POST',
                              data: {
                                  booking_id: bid,getBookingDetail:'book_detail'
                              },
                              success: function(response_data) {
                                  $("#myModal_Data_Post .modal-header .modal-title").html("Booking Detail");
                               //   $("#myModal_Data_Post").css("display","block");
                                 // $("#myModal_Data_Post").css("opacity","1");
                                  $("#myModal_Data_Post .modal-body").html(response_data);
                                  $("#myModal_Data_Post").modal();
                                  //$(".modal-backdrop").remove();
                              }
                            });
                    });



                    jQuery(document).on("click", ".RequestView_Detail", function(e){
                        
                        var b_url="";
                         e.preventDefault();
                        
                        var id=$(this).attr("data-value");
                        var type=$(this).attr("data-view_type");
                        if(type=="Booking"){

                            $("#myModal_Data_Post .modal-title").html("Booking Detail");
                            b_url=baseURL+"Booking/booking_detail_view";
                        }
                        else if(type=="contact_enquiry"){

                            $("#myModal_Data_Post .modalt-itle").html("Contact Detail");
                            b_url=baseURL+"Administrator/contact_enquiry_view";
                        }
                        else
                        
                            b_url="";

                            if(b_url!="")
                            {
                                jQuery.ajax({
                                    
                                    type : "POST",
                                    dataType : "json",
                                    url : b_url,
                                    data : { enquiry_id : id } 
                                    }).done(function(data){

                                        if(type=="contact_enquiry" && data.status)
                                        {
                                             recordDisplayjsontoHtml(data.data,"Request-modal-model-body");
                                            $('#myModal_Data_Post').modal('show'); 
                                        }else{
                                                recordDisplayjsontoHtml(data,"Request-modal-model-body");
                                                $('#myModal_Data_Post').modal('show'); 
                                        }

                                        
                                });

                            }
                        

                    });

                // booking reponse action

                jQuery(document).on("click", ".booking_response_action", function(e){
                        
                        var b_url="";
                        e.preventDefault();
                    
                        var id=$(this).attr("data-value");
                        var type=$(this).attr("data-view_type");
                        var action=$(this).html();
                        var current=$(this);

                        if(type=="Booking"){

                            $("#myModal_Data_Post .modal-title").html("Booking Detail");
                            b_url=baseURL+"Booking/updae_booking_status";
                        }
                        else if(type=="contact_enquiry"){

                            $("#myModal_Data_Post .modalt-itle").html("Contact Detail");
                            b_url=baseURL+"Administrator/update_contact_enquir_status";

                        }
                        else
                            b_url="";

                            if(b_url!="")
                            {
                                jQuery.ajax({
                                    
                                    type : "POST",
                                    dataType : "json",
                                    url : b_url,
                                    data : { enquiry_id : id,action_status:action } 
                                    }).done(function(data){
                                        if(!data.error)
                                        {
                                                if(type=="contact_enquiry"){

                                                     var a=$(".contact_enquirry_request_count").html();
                                                    a--;
                                                    $(".contact_enquirry_request_count").html(a);

                                                }else{
                                                     var a=$(".booking_request_count").html();
                                                    a--;
                                                    $(".booking_request_count").html(a);     
                                                }
                                               
                                                current.parentsUntil(".request_Block").remove();
                                        }else{
                                            alert("error in Updation");
                                        }
                                });
                            }
                    });
            });
            
            var windowURL = window.location.href;
            pageURL = windowURL.substring(0, windowURL.lastIndexOf('/'));
            var x= $('a[href="'+pageURL+'"]');
                x.addClass('active');
                x.parent().addClass('active');
            var y= $('a[href="'+windowURL+'"]');
                y.addClass('active');
                y.parent().addClass('active');

                function recordDisplayjsontoHtml(json_data,div_id) {
        
                    var tr="";
                    var table="";
                    for(p in json_data)
                    {
                        tr+="<tr><th>"+p+"</th><td>"+json_data[p]+"</td></tr>"; 
                    }
                    table="<table class='table table-responsive'>"+tr+"</table>";
                    $("#"+div_id).html(table);
                }

        </script>

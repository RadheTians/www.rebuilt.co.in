<script>
    // appliances services : sidebar fixed on scroll
(function($) {
          $(function() {

              var sidebar = $('.user-profile-box');
              var top = sidebar.offset().top - parseFloat(sidebar.css('margin-top'));
              var intro_section=$(".intro-section");

              $(window).scroll(function (event) {
               var height=sidebar.height();
                var y = $(this).scrollTop();
                if (y >= top && (y+height)<intro_section.offset().top) {
                  sidebar.css({position:"fixed"});
                  sidebar.addClass("width_sidebar");
                } else {
                      sidebar.css({position:"relative","width":"100%"});
                       sidebar.removeClass("width_sidebar");
                }
              });
              
                  // smooth scroll
               $("div.user-profile-box .detail a").on('click', function(event) {

                 $("div.user-profile-box .detail a").removeClass("active_color");
                  $(this).addClass("active_color");

                  // Make sure this.hash has a value before overriding default behavior
                  if (this.hash !== "") {
                    // Prevent default anchor click behavior
                    event.preventDefault();

                    // Store hash
                    var hash = this.hash;

                    // Using jQuery's animate() method to add smooth page scroll
                    // The optional number (800) specifies the number of milliseconds it takes to scroll to the specified area
                    $('html, body').animate({
                      scrollTop: $(hash).offset().top
                    }, 800, function(){

                      // Add hash (#) to URL when done scrolling (default click behavior)
                      window.location.hash = hash;
                    });
                  } // End if
                });

                $(window).scroll(function(){
               var y = $(this).scrollTop();
               if (y > 100) {
                     $(".st1").css("display","none");
                     $(".header_style_1").css("width","100%");
                  } else {
                     $(".header_style_1").css("position","relative");
               }
            });
     });
  })(jQuery);
</script>
